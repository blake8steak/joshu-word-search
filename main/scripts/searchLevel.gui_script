				nodeStringOne = nodeStringOne..gui.get_text(gui.get_node("letter"..(node1Col).."-"..i))
			end
		else
			-- col moves right /^
			for i=node1Row, node2Row do
				-- i is getting bigger
				--print("===LETTER: "..gui.get_text(gui.get_node("letter"..(node1Col+(i-node1Row)).."-"..i)))
				nodeStringOne = nodeStringOne..gui.get_text(gui.get_node("letter"..(node1Col+(i-node1Row)).."-"..i))
			end
		end
	end 
	return nodeStringOne
