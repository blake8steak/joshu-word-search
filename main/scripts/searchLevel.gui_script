function on_message(self, message_id, message, sender)
	-- Add level details here 
	if message_id == hash("init_tiles") then
		print("Initializing tiles...")
		wordBank = message
		--print("=== DEFINITION: "..wordBank.def[wordBank.words[1]])
		for k in pairs(wordBank.words) do 
			wordBankSize = wordBankSize + 1
		end
		local bank = {}
		math.randomseed(os.clock()*100000000000)
		shuffle(wordBank.words)
		shuffle(wordBank.def)
		shuffle(wordBank)
		place_word(0, bank)
		change_word_hint(0)
		fill_in_empty_boxes()
	end
	if message_id == hash("tile_selected") then

	end
end

