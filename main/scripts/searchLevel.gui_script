		else
			for i=node1Col, node2Col do
				-- i is getting bigger
				--print("===LETTER: "..gui.get_text(gui.get_node("letter"..i.."-"..node1Row)))
				nodeStringOne = nodeStringOne..gui.get_text(gui.get_node("letter"..i.."-"..node1Row))
			end
		end
	else
		-- word starts high, moves down
		-- row doesn't say anything about column
		--    (could increase, stay same, decrease)
		if node1Col > node2Col then
			-- col moves left ^\
			for i=node1Row, node2Row do
				-- i is getting bigger
				--print("===LETTER: "..gui.get_text(gui.get_node("letter"..(node1Col-(i-node1Row)).."-"..i)))
