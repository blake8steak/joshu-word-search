function setBoxData(boxCol, boxRow, boxChar, boxName)
	local nodesAreValid = false
	msg.post("/searchLevel#searchScript", "tile_selected", {col = boxCol, row = boxRow, char = boxChar})
	if node1 == nil then
		node1 = gui.get_node(boxName)
		node1Col = boxCol
		node1Row = boxRow
		node1Char = boxChar
		highlight_box(node1)
	else
		if node2 == nil then
			node2 = gui.get_node(boxName)
			node2Col = boxCol
			node2Row = boxRow
			node2Char = boxChar
			if check_if_valid_nodes() == true then 
				-- if in word bank
				--       | highlight all boxes in between these two
				--       | mark word as found

				-- highlight_box(node2)

				-- highlight_valid()
				nodesAreValid = true
				unhighlight_valid(nodesAreValid)
			else
				-- unhighlight both
				-- null both
				nodesAreValid = false
				-- unhighlight_box(node1, nodesAreValid)
				-- unhighlight_box(node2, nodesAreValid)
				unhighlight_valid(nodesAreValid)
				node1 = nil
				node2 = nil
			end
		else
			-- both node1 and node2 set
			--
			-- would not be a normal case, highlights are nulled
			-- upon node2 selection and checks 
			-- (either word exists and marked green, or it doesn't - both are nulled)
			nodesAreValid = check_if_valid_nodes()
			unhighlight_valid(nodesAreValid)
			node1 = nil
			node2 = nil
			setBoxData(boxCol, boxRow, boxChar, boxName)
		end
	end
end

