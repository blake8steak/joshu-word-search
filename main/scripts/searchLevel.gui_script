		end
	elseif node1Row == node2Row then
		-- figure out if col moves left or right
		if node1Col > node2Col then
			for i=node2Col, node1Col do
				-- i is getting bigger
				--print("===LETTER: "..gui.get_text(gui.get_node("letter"..i.."-"..node1Row)))
