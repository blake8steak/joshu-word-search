		for i = lowCol, highCol do
			unhighlight_box(gui.get_node("tile"..i.."-"..lowRow), nodesAreValid)
		end
	elseif highCol == lowCol then
		-- same col
		for i = lowRow, highRow do
			unhighlight_box(gui.get_node("tile"..lowCol.."-"..i), nodesAreValid)
		end
	else
		if math.abs(node1Row-node2Row) ~= math.abs(node1Col-node2Col) then 
