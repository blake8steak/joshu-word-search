	if action.pressed and gui.pick_node(gui.get_node("leftArrow"), action.x, action.y) then
		print("Left arrow pressed: ")
		change_word_hint(-1)
	end
	if action.pressed and gui.pick_node(gui.get_node("rightArrow"), action.x, action.y) then
		print("Right arrow pressed")
		change_word_hint(1)
	end
end

function change_word_hint(shiftNum)
	-- shiftNum in initial call is 0
	-- else shiftNum is always either +1 or -1
	
	-- keep index in bounds
	if (wordHintIndex+shiftNum) > wordBankSize then
		wordHintIndex = 1
	elseif (wordHintIndex+shiftNum) == 0 then
		wordHintIndex = wordBankSize
	else 
		wordHintIndex = (wordHintIndex+shiftNum)
	end

	-- update gui info
	gui.set_text(gui.get_node("wordNumber"), "ワード #"..wordHintIndex)
	gui.set_text(gui.get_node("wordHint"), wordBank.def[wordBank.words[wordHintIndex]])
end

