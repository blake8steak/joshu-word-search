function handle_valid_highlight()
	local nodeStringOne = get_highlighted_string()
	local nodeStringTwo = ""
	local thisIsRevTable = {}
	local index = 0
	-- need to check highlighted word backwards and forwards
	-- reverse string, make it work with japanese chars
	for c in nodeStringOne:gmatch("[%z\1-\127\194-\244][\128-\191]*") do
		thisIsRevTable[#thisIsRevTable+1] = c
	end
	for i = #thisIsRevTable, 1, -1 do
		nodeStringTwo = nodeStringTwo..thisIsRevTable[i]
	end
	--print("HIGHLIGHTED WORD: \n"..nodeStringOne.."\n, "..nodeStringTwo)

	-- check if words exist in wordBank
	local checkWord = ""
	local foundWord = false
	for i = 1, #wordBank.words do
		checkWord = wordBank.words[i]
		if checkWord == nodeStringOne or checkWord == nodeStringTwo then
			print("=== FOUND VALID WORD! "..checkWord)
			foundWord = true
			-- Handle all found word functionality here
			return true
		end
	end
	if not foundWord then 
		print("=== Not a valid word.")
		-- prevent unhighlight!!
		return false
	end
end

