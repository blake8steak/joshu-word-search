			end
		elseif node1Col > node2Col and node1Row > node2Row then
			-- c1 > c2
			-- r1 > r2
			-- [2][ ][ ]
			-- [ ][X][ ]
			-- [ ][ ][1]
			for i=0, numIter do
				unhighlight_box(gui.get_node("tile"..(highCol-i).."-"..(highRow-i)), nodesAreValid)
			end
		elseif  node1Col < node2Col and node1Row > node2Row then
